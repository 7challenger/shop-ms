{
  "name": "parsers-services",
  "version": "0.0.1",
  "author": "kurasixo",
  "private": true,
  "scripts": {
    "test": "echo test",
    "lint": "echo lint",

    "build:docker-api-gateway": "docker build --build-arg host=http://chabox.ru -t api-gateway:0.0.1 -f ./Api/Dockerfile .",
    "build:docker-site-checker": "docker build -t site-checker:0.0.1 -f ./SiteChecker/Dockerfile .",
    "build:docker-parser": "docker build -t parser:0.0.1 -f ./Parser/Dockerfile .",

    "tag:docker-api-gateway": "docker tag api-gateway:0.0.1 repo.treescale.com/seven-challenger/api-gateway:0.0.1",
    "tag:docker-site-checker": "docker tag site-checker:0.0.1 repo.treescale.com/seven-challenger/site-checker:0.0.1",
    "tag:docker-parser": "docker tag parser:0.0.1 repo.treescale.com/seven-challenger/parser:0.0.1",

    "publish:docker-api-gateway": "yarn tag:docker-api-gateway && docker push repo.treescale.com/seven-challenger/api-gateway:0.0.1",
    "publish:docker-site-checker": "yarn tag:docker-site-checker && docker push repo.treescale.com/seven-challenger/site-checker:0.0.1",
    "publish:docker-parser": "yarn tag:docker-parser && docker push repo.treescale.com/seven-challenger/parser:0.0.1",

    "run:docker-api-gateway": "docker run --name api-gateway -d api-gateway:0.0.1",
    "run:docker-site-checker": "docker run --name site-checker -d site-checker:0.0.1",
    "run:docker-parser": "docker run --name parser -d parser:0.0.1",

    "run:compose": "docker-compose -f docker-compose.dev.yml up --build"
  },
  "dependencies": {
    "amqplib": "^0.5.6",
    "archiver": "^3.1.1",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "diff-dom": "^4.1.6",
    "express": "^4.17.1",
    "graylog2": "^0.2.1",
    "mongoose": "^5.9.18",
    "net": "^1.0.2",
    "node-fetch": "^2.6.0",
    "puppeteer": "^3.3.0",
    "tls": "^0.0.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.6",
    "@types/graylog2": "^0.2.0",
    "@types/lodash": "^4.14.155",
    "@types/mongoose": "^5.7.24",
    "@types/puppeteer": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "bufferutil": "^4.0.1",
    "eslint": "^7.2.0",
    "source-map-loader": "^1.0.0",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "utf-8-validate": "^5.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
